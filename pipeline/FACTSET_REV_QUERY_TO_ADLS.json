{
	"name": "FACTSET_REV_QUERY_TO_ADLS",
	"properties": {
		"activities": [
			{
				"name": "Execute RAW_TO_DELTA",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Set guid variable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "RAW_TO_DELTA",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"filePath": {
							"value": "@concat('/mnt/', pipeline().parameters.filePath)",
							"type": "Expression"
						},
						"dataSource": {
							"value": "@pipeline().parameters.dataSource",
							"type": "Expression"
						},
						"dataSet": {
							"value": "@pipeline().parameters.dataSet",
							"type": "Expression"
						},
						"guid": {
							"value": "@variables('guid')",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "Set guid variable",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "guid",
					"value": {
						"value": "@if(empty(pipeline().parameters.guid), guid(), pipeline().parameters.guid)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Feed Issuer revenue metric in business",
				"type": "DatabricksSparkJar",
				"dependsOn": [
					{
						"activity": "Execute RAW_TO_DELTA",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"mainClassName": "com.carmignac.icare.elysee.project.business.IssuerRevenueMetric",
					"parameters": [
						"@pipeline().parameters.filePath",
						"@pipeline().parameters.dataSource",
						"@pipeline().parameters.dataSet",
						"@variables('guid')"
					],
					"libraries": [
						{
							"jar": "dbfs:/databricks/job-jars/IcareDataLib.jar"
						}
					]
				},
				"linkedServiceName": {
					"referenceName": "LS_DTBK_1_NODE_12_2_RUNTIME",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"filePath": {
				"type": "string"
			},
			"dataSource": {
				"type": "string",
				"defaultValue": "factset_rev"
			},
			"dataSet": {
				"type": "string",
				"defaultValue": "databricks_bus_seg_report_item_delta"
			},
			"guid": {
				"type": "string"
			}
		},
		"variables": {
			"guid": {
				"type": "String"
			},
			"day": {
				"type": "String"
			}
		},
		"folder": {
			"name": "ISSUER"
		},
		"annotations": []
	}
}