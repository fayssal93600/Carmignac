{
	"name": "SAM_GENERIC_TABLE_TO_ADLS",
	"properties": {
		"activities": [
			{
				"name": "Copy SAM SQL TO ADLS 2",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SAM_Generic",
						"type": "DatasetReference",
						"parameters": {
							"tableName": {
								"value": "@pipeline().parameters.tableName",
								"type": "Expression"
							},
							"schemaName": {
								"value": "@pipeline().parameters.schemaName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_INPUT_GENERIC_FILENAME",
						"type": "DatasetReference",
						"parameters": {
							"tableName": {
								"value": "@pipeline().parameters.tableName",
								"type": "Expression"
							},
							"sourceName": {
								"value": "@variables('sourceName')",
								"type": "Expression"
							},
							"fileName": {
								"value": "@concat('in/',utcNow('yyyyMMdd'),'/',utcNow('yyyyMMdd_HHmmss'),'_',pipeline().parameters.tableName,'.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"schemaName": {
				"type": "string"
			},
			"tableName": {
				"type": "string"
			}
		},
		"variables": {
			"sourceName": {
				"type": "String",
				"defaultValue": "sam"
			}
		},
		"folder": {
			"name": "SAM"
		},
		"annotations": [],
		"lastPublishTime": "2022-05-05T08:03:08Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}